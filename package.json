{
  "name": "Weather-app",
  "version": "0.1.0",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ios": "npx cap open ios",
    "ios:copy": "npx ionic capacitor copy ios",
    "ios:hot": "npx ionic capacitor run ios -l --external",
    "android": "npx cap open android",
    "android:copy": "npx ionic capacitor copy android",
    "nuke": "rm -rf node_modules && npm install",
    "build:android": "ionic cordova build android",
    "build:android-apk": "ionic cordova build android --prod --release",
    "build:android-aab": "cd platforms/android && ./gradlew bundle",
    "sign:android-aab": "jarsigner -verbose -tsa http://timestamp.digicert.com -tsacert panahon -sigalg SHA256withRSA -digestalg SHA-256 -keystore platforms/android/panahon-release-key-new platforms/android/app/build/outputs/bundle/release/app.aab panahon",
    "sign:android-apk": "jarsigner -verbose -tsa http://timestamp.digicert.com -tsacert panahon -sigalg SHA256withRSA -digestalg SHA-256 -keystore platforms/android/panahon-release-key-new platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk panahon"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@capacitor/android": "^2.3.0",
    "@capacitor/core": "2.3.0",
    "@capacitor/ios": "^2.3.0",
    "@ionic-native/core": "^5.17.1",
    "@ionic-native/geolocation": "^5.17.1",
    "@ionic-native/native-geocoder": "^5.17.1",
    "@ionic-native/open-native-settings": "^5.17.1",
    "@ionic-native/splash-screen": "^5.17.1",
    "@ionic-native/status-bar": "^5.17.1",
    "@ionic/angular": "^4.11.5",
    "@ionic/storage": "^2.2.0",
    "cordova-android": "^8.1.0",
    "cordova-browser": "^5.0.4",
    "cordova-open-native-settings": "1.5.2",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-ionic-webview": "^4.1.3",
    "cordova-plugin-nativegeocoder": "3.2.2",
    "cordova-plugin-whitelist": "^1.3.4",
    "core-js": "^2.6.10",
    "ionic-cache": "^4.1.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.13.9",
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular-devkit/core": "^7.3.9",
    "@angular-devkit/schematics": "^7.3.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler": "^7.2.15",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@capacitor/cli": "2.3.0",
    "@ionic/angular-toolkit": "~1.5.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^10.14.22",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "^8.1.1",
    "tslint": "~5.16.0",
    "typescript": "~3.1.6"
